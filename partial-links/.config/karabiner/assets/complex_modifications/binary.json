{
	"title": "Execute Binary",
	"rules": [
		{
			"description": "Toggle Mission Control with full space bar",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "m",
						"modifiers": {
							"mandatory": [
								"left_option"
							]
						}
					},
					"to": [
						{
							"shell_command": "~/Dev/prod/desktopApp/clang/missionControl/a.out"
						}
					]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "m",
						"modifiers": {
							"mandatory": [
								"right_option"
							]
						}
					},
					"to": [
						{
							"shell_command": "~/Dev/prod/desktopApp/clang/missionControl/a.out"
						}
					]
				}
			]
		}
	]
}
