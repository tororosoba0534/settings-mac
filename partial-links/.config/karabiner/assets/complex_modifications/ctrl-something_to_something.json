{
	"title": "CTRL-something to something",
	"rules": [
		{
			"description": "CTRL-g to ESC",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "g",
						"modifiers": {
							"mandatory": [
								"control"
							],
							"optional": [
								"caps_lock",
								"option"
							]
						}
					},
					"to": [
						{
							"key_code": "escape"
						}
					],
					"conditions": [
						{
							"type": "frontmost_application_unless",
							"bundle_identifiers": [
								"^com\\.apple\\.Terminal$",
								"^com\\.googlecode\\.iterm2$",
								"^org\\.alacritty$"
							]
						}
					]
				}
			]
		},
