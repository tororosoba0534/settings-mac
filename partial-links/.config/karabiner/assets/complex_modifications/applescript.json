{
	"title": "AppleScript",
	"rules": [
		{
			"description": "Launch new Chrome window",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "b",
						"modifiers": {
							"mandatory": [
								"left_command",
								"left_shift"
							]
						}
					},
					"to": [
						{
							"shell_command": "osascript ~/settings-mac/applescript/launch_chrome_new_window.applescript"
						}
					]
				}
			]
		},
		{
			"description": "Toggle Alacritty",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "spacebar",
						"modifiers": {
							"mandatory": [
								"left_option"
							]
						}
					},
					"to": [
						{
							"shell_command": "osascript ~/settings-mac/applescript/toggle_alacritty.applescript"
						}
					]
				}
			]
		},
		{
			"description": "Toggle Terminal",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "spacebar",
						"modifiers": {
							"mandatory": [
								"left_option",
								"left_shift"
							]
						}
					},
					"to": [
						{
							"shell_command": "osascript ~/settings-mac/applescript/toggle_terminal.applescript"
						}
					]
				}
			]
		},
		{
			"description": "Rotate Chrome Windows (Reverse)",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "b",
						"modifiers": {
							"mandatory": [
								"left_option"
							]
						}
					},
					"to": [
						{
							"shell_command": "osascript ~/settings-mac/applescript/rotate_chrome_windows_reverse.applescript"
						}
					]
				}
			]
		}
	]
}
