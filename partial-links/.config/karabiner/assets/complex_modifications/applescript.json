{
	"title": "AppleScript",
	"rules": [
		{
			"description": "AppleScript",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "b",
						"modifiers": {
							"mandatory": [
								"left_command",
								"left_shift"
							]
						}
					},
					"to": [
						{
							"shell_command": "osascript ~/settings-mac/applescript/launch_chrome_new_window.applescript"
						}
					]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "b",
						"modifiers": {
							"mandatory": [
								"left_option"
							]
						}
					},
					"to": [
						{
							"shell_command": "osascript ~/settings-mac/applescript/rotate_chrome_windows_reverse.applescript"
						}
					]
				}
			]
		}
	]
}
