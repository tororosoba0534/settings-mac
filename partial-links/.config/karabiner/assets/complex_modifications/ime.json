{
	"title": "custom IME setting",
	"rules": [
		{
			"description": "LCMD -> EISUU, RCMD -> KANA, ROPT -> KANA",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "left_command",
						"modifiers": {
							"optional": [
								"any"
							]
						}
					},
					"parameters": {
						"basic.to_if_held_down_threshold_milliseconds": 100
					},
					"to": [
						{
							"key_code": "left_command",
							"lazy": true
						}
					],
					"to_if_held_down": [
						{
							"key_code": "left_command"
						}
					],
					"to_if_alone": [
						{
							"key_code": "japanese_eisuu"
						}
					]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "right_command",
						"modifiers": {
							"optional": [
								"any"
							]
						}
					},
					"parameters": {
						"basic.to_if_held_down_threshold_milliseconds": 100
					},
					"to": [
						{
							"key_code": "right_command",
							"lazy": true
						}
					],
					"to_if_held_down": [
						{
							"key_code": "right_command"
						}
					],
					"to_if_alone": [
						{
							"key_code": "japanese_kana"
						}
					]
				},
				{
					"type": "basic",
					"from": {
						"key_code": "right_option",
						"modifiers": {
							"optional": [
								"any"
							]
						}
					},
					"parameters": {
						"basic.to_if_held_down_threshold_milliseconds": 100
					},
					"to": [
						{
							"key_code": "right_option",
							"lazy": true
						}
					],
					"to_if_held_down": [
						{
							"key_code": "right_option"
						}
					],
					"to_if_alone": [
						{
							"key_code": "japanese_kana"
						}
					]
				}
			]
		}
	]
}
